<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >
    <ImageView
        android:id="@+id/c1"
        android:layout_width="46dp"
        android:layout_height="wrap_content"
        android:src="@drawable/key_left" />

    <ImageView
        android:id="@+id/d1"
        android:layout_width="46dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/c1"
        android:src="@drawable/key_sym" />

    <ImageView
        android:id="@+id/e1"
        android:layout_width="46dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/d1"
        android:src="@drawable/key_right" />

    <ImageView
        android:id="@+id/cs1"
        android:layout_width="30dp"
        android:layout_height="180dp"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/d1"
        android:gravity="top"
        android:scaleType="fitXY"
        android:src="@drawable/key_black" />

    <ImageView
        android:id="@+id/ds1"
        android:layout_width="30dp"
        android:layout_height="180dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/d1"
        android:gravity="top"
        android:scaleType="fitXY"
        android:src="@drawable/key_black" />

    <ImageView
        android:id="@+id/f1"
        android:layout_width="46dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/e1"
        android:src="@drawable/key_left" />

</RelativeLayout>